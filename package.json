{
  "name": "physipro-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:safe": "node scripts/check-dev-db.js && ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest --testPathPattern=unit",
    "test:unit": "jest --testPathPattern=unit",
    "test:unit:watch": "jest --testPathPattern=unit --watch",
    "test:integration": "node scripts/check-db-connection.js && jest src/tests/integration/dummy.test.ts",
    "test:integration:docker": "chmod +x ./docker/test/run-integration-tests.sh && ./docker/test/run-integration-tests.sh",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "jest --testPathPattern=unit --coverage",
    "test:coverage:integration": "node scripts/check-db-connection.js && jest src/tests/integration/dummy.test.ts --coverage",
    "test:coverage:docker": "chmod +x ./docker/test/run-integration-tests.sh && ./docker/test/run-integration-tests.sh --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:start": "chmod +x ./docker/dev/docker-start.sh && ./docker/dev/docker-start.sh",
    "docker:stop": "docker-compose -f docker/dev/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/dev/docker-compose.yml logs -f postgres",
    "init:admin": "ts-node src/scripts/init-admin.ts",
    "typecheck": "tsc --noEmit",
    "cleanup:js": "./cleanup-js-files.sh",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/compression": "^1.7.5",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.14.1",
    "sequelize": "^6.37.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.17",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "bcryptjs": "^3.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
